{
  "name": "robotdance/php-app-config",
  "type": "library",
  "description": "A PHP helper component to store package/application settings",
  "keywords": ["php", "config", "settings", "component", "yaml", "yml"],
  "license": "MIT",
  "authors": [{
    "name": "Marcos Menegazzo",
    "email": "marcos.menegazzo@gmail.com"
  }],
  "require": {
    "php": ">=5.3.2",
    "symfony/yaml": "~3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.24",
    "phpdocumentor/phpdocumentor": "2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "robotdance\\": "src",
      "robotdance\\tests\\": "tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "robotdance\\tests\\": "tests"
    }
  }
}
